{
  "id": "openclaw-self-healing",
  "name": "OpenClaw Self Healing",
  "version": "0.1.0",
  "description": "Self-healing health checks + guardrails + auto-fix for reversible failures (rate limits, auth errors, stuck session pins).",
  "configSchema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "enabled": { "type": "boolean", "default": true },
      "modelOrder": {
        "type": "array",
        "items": { "type": "string" },
        "default": [
          "anthropic/claude-opus-4-6",
          "openai-codex/gpt-5.2",
          "google-gemini-cli/gemini-2.5-flash"
        ]
      },
      "cooldownMinutes": { "type": "number", "default": 300, "minimum": 1 },
      "stateFile": { "type": "string", "default": "~/.openclaw/workspace/memory/self-heal-state.json" },
      "sessionsFile": { "type": "string", "default": "~/.openclaw/agents/main/sessions/sessions.json" },
      "autoFix": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "patchSessionPins": { "type": "boolean", "default": true },
          "disableFailingPlugins": { "type": "boolean", "default": false },
          "disableFailingCrons": { "type": "boolean", "default": false }
        }
      }
    }
  }
}
